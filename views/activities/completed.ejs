<h4>Completed: </h4>
<% saved.forEach(save => { %>
    <% if(user.id === save.userId) {%>
    <% if(save.completed) {%>
        <ul>
        <li><%= save.activity%></li>
        <form method="POST" action ="/saved/<%= save.id%>/?_method=DELETE">
            <input type="submit" value="delete">
        </form>
        <a href="/recommendations/new/<%=save.id%>">Click to Post recommendation!</a>
        </ul>
    <%}}%>        
<%})%>